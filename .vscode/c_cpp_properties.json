{
  "configurations": [
    {
      "name": "MSVC",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/src",
        "C:/Qt/*/include/**",
        "C:/Qt/*/msvc*/include/**",
        "C:/Qt/6.8.0/msvc2022_64/include/**",
        "C:/Qt/6.7.0/msvc2022_64/include/**",
        "C:/Qt/6.6.0/msvc2022_64/include/**",
        "C:/Qt/6.5.3/msvc2019_64/include/**",
        "C:/Qt/6.5.0/msvc2019_64/include/**",
        "C:/Qt/6.5.0/msvc2022_64/include/**"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "WIN32",
        "_WIN32",
        "QT_CORE_LIB",
        "QT_GUI_LIB",
        "QT_WIDGETS_LIB",
        "QT_OPENGLWIDGETS_LIB"
      ],
      "compilerPath": "C:/Program Files/Microsoft Visual Studio/2022/Community/VC/Tools/MSVC/14.*/bin/Hostx64/x64/cl.exe",
      "cStandard": "c17",
      "cppStandard": "c++17",
      "intelliSenseMode": "msvc-x64",
      "compileCommands": "${workspaceFolder}/build/debug/compile_commands.json",
      "configurationProvider": "ms-vscode.cmake-tools",
      "browse": {
        "path": [
          "${workspaceFolder}/**",
          "C:/Qt/*/include/**"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ],
  "version": 4
}
